@model Microsoft.AspNetCore.Identity.IdentityUser

@{
    ViewData["Title"] = "Permission";
}

<h1>Permission</h1>

<h3>Are you sure you want to change this user's permission?</h3>
<div>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt class="col-sm-2">
            E-Mail
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UserName)
        </dd>
    </dl>

    <form asp-action="Permission">
        <input type="submit" value="Change Permission" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>