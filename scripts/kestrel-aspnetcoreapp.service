[Unit]
Description=Access analyser ASP.NET Core Web Application

[Service]
WorkingDirectory=/home/ec2-user/accessanalyserapp
ExecStart=/usr/bin/dotnet /home/ec2-user/accessanalyserapp/AccessAnalyser.dll
Restart=always
# Restart service after 10 seconds if dotnet service crashes
RestartSec=10
SyslogIdentifier=accessanalyser
User=www-data
Environment=ASPNETCORE_ENVIRONMENT=Production 

[Install]
WantedBy=multi-user.target
